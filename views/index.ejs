<%- include header%>
    <% if (success) { %>
       <div><%= success %></div>
    <% } %>
    <% if (error) { %>
       <div><%= error %></div>
    <% } %>
    <h1>这是主页！</h1>
	<% posts.forEach(function (post, index) { %>
  		<p><h2><a href="#"><%= post.title %></a></h2></p>
  		<p class="info">
   		作者：<a href="#"><%= post.name %></a> |
    		日期：<%= post.time.minute %>
  		</p>
  		<p><%- post.post %></p>
	<% }) %>
<%- include footer%>
